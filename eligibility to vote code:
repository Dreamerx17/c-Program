#include <iostream>

using namespace std;

int  main()
 {
 	int age;
 	cout<<" Enter your Age pls: ";
 	cin>>age;
 	
 	if( age>18){
 		
 		cout<<" \n Your  definitely eligible to vote :-)\n";
	 }
	 else if(age==18){
	 	
	 	cout<< "\n Congratulation Your  eligible to vote :-D\n";
	 }
	 else  {
	 cout<< "\nSorry you are not eligible to vote :-(.\n";
	 cout<< "\nTo vote you have to be 18 or more :O\n";
	cout<< "\n Better luck next time ;-)";
}
return 0;
}
